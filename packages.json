{
    "babel": {
        "plugins": [
          [
            "lodash",
            {
              "id": [
                "lodash",
                "recompose"
              ]
            }
          ],
          [
            "module-resolver",
            {
              "root": [
                "./"
              ],
              "alias": {
                "@agnuscrm/reaction-collections": "./imports/plugins/core/collections",
                "@agnuscrm/reaction-components": "./imports/plugins/core/components/lib",
                "@agnuscrm/reaction-router": "./imports/plugins/core/router/lib",
                "@agnuscrm/reaction-ui": "./imports/plugins/core/ui/client/components",
                "underscore": "lodash"
              }
            }
          ]
        ],
        "presets": [
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions",
                  "safari >= 7"
                ]
              }
            }
          ],
          [
            "react"
          ],
          [
            "stage-2"
          ]
        ]
      },
}